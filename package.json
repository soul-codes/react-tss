{
  "name": "react-tss",
  "version": "0.1.7-rc.1",
  "repository": {
    "url": "https://github.com/soul-codes/react-tss"
  },
  "scripts": {
    "dev": "./bin/dev.sh",
    "ts": "ttsc -b -w src/demo/main",
    "prepare": "ttsc -b src/lib/tsconfig.lib.json",
    "demo:build": "ttsc -b src/demo/main src/demo/webpack && . ./bin/build-demo.sh && cpy src/demo/webpack/public/index.html target/web/",
    "test": "jest",
    "release": "release-it",
    "tidy": "./bin/tidy.sh"
  },
  "main": "lib",
  "type": "module",
  "files": [
    "lib/**",
    "src/lib/**"
  ],
  "license": "MIT",
  "dependencies": {
    "hash-sum": "^2.0.0",
    "jss": "^10.9.0",
    "jss-preset-default": "^10.9.0",
    "lru-cache": "^6.0.0",
    "reffx": "1.0.0-rc.0"
  },
  "devDependencies": {
    "@release-it/conventional-changelog": "^1.1.0",
    "@soul-codes-dev/typetools": "^0.3.0",
    "@types/copy-webpack-plugin": "^6.0.0",
    "@types/express": "^4.17.8",
    "@types/hash-sum": "^1.0.0",
    "@types/jest": "^24.0.25",
    "@types/lodash-es": "^4.17.3",
    "@types/lru-cache": "^5.1.0",
    "@types/nanoid": "^2.1.0",
    "@types/node": "^14.11.8",
    "@types/react": "^16.9.17",
    "@types/react-dom": "^16.9.4",
    "@types/react-icons": "^3.0.0",
    "@types/react-router-dom": "^5.1.5",
    "@types/webpack": "^4.41.22",
    "@types/webpack-bundle-analyzer": "^3.8.0",
    "@types/webpack-dev-server": "^3.11.0",
    "@types/webpack-merge": "^4.1.5",
    "@zerollup/ts-transform-paths": "^1.7.18",
    "@zoltu/typescript-transformer-append-js-extension": "^1.0.1",
    "app-root-dir": "^1.0.2",
    "clsx": "^1.1.1",
    "copy-webpack-plugin": "^6.1.1",
    "cpy-cli": "^3.1.1",
    "cross-env": "^7.0.2",
    "csstype": "^3.0.6",
    "esm": "^3.2.25",
    "fast-json-stringify": "^2.4.1",
    "fp-ts": "^2.8.4",
    "import-sort-style-module": "^6.0.0",
    "jest": "^24.9.0",
    "lodash-es": "^4.17.15",
    "nodemon": "^2.0.4",
    "path-to-regex": "^1.3.4",
    "pkg-dir": "^5.0.0",
    "preact": "^10.4.4",
    "preact-compat": "^3.19.0",
    "prettier": "^2.2.1",
    "prettier-plugin-import-sort": "^0.0.5",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-icons": "^3.11.0",
    "react-jss": "^10.5.1",
    "react-router-dom": "^5.2.0",
    "release-it": "^12.4.3",
    "source-map-loader": "^1.1.2",
    "ts-jest": "^24.3.0",
    "ts-loader": "^6.2.1",
    "ts-node": "^9.0.0",
    "ts-typetools": "^0.5.1",
    "tsconfig-paths-webpack-plugin": "^3.3.0",
    "tslint": "^6.1.2",
    "tslint-etc": "^1.11.0",
    "ttypescript": "^1.5.12",
    "typescript": "^4.1.4",
    "webpack": "^4.44.1",
    "webpack-bundle-analyzer": "^3.8.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.2",
    "webpack-merge": "^4.2.2"
  },
  "release-it": {
    "plugins": {
      "@release-it/conventional-changelog": {
        "preset": "angular",
        "infile": "CHANGELOG.md"
      }
    }
  },
  "importSort": {
    ".js, .jsx, .ts, .tsx": {
      "style": "module",
      "parser": "typescript"
    }
  }
}
